{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"pdc": {
			"type": "object",
			"required": [
				"ifscCode"
			],
			"properties": {
				"loanAccountNumber": {
					"type": "string",
					"title": "LOAN_ACCOUNT_NUMBER"
				},
				"accountNumber": {
					"type": "string",
					"title": "LOAN_ACCOUNT_NUMBER"
				},
				"reference": {
					"type": "string",
					"title": "INSTRUMENT_REFERENCE"
				},
				"ifscCode": {
					"type": "string",
					"title": "IFSC_CODE"
				},
				"bankName": {
					"type": "string",
					"title": "BANK_NAME"
				},
				"branchName": {
					"type": "string",
					"title": "BRANCH_NAME"
				},
				"addCheque": {
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"ifscCode"
						],
						"properties": {
							"ifscCode": {
								"type": "string",
								"title": "IFSC_CODE"
							},
							"bankName": {
								"type": "string",
								"title": "BANK_NAME"

							},
							"branchName": {
								"type": "string",
								"title": "BRANCH_NAME"
							},
							"chequeNoFrom":{
								"type": "string",
								"title": "CHEQUE_START_NUMBER",
								"minLength": 6,
                                "maxLength": 6
							}
						}
					}
				}
			}
		}
	}
}