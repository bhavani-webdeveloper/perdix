{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "disbursementProcessAction": {
      "type": "string"
    },
    "customer":{
      "type":"object",
      "properties":{
        "customerId":{
          "type":"string",
          "title":"ENTITY_ID"
        },
        "customerUrnNo":{
          "type":"string",
          "title":"ENTITY_URN"
        }

      }

    },
    "individualLoanDocuments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "disbursementId": {
            "type": "integer"
          },
          "document": {
            "type": "string"
          },
          "documentId": {
            "type": "string"
          },
          "documentStatus": {
            "type": "string"
          },
          "id": {
            "type": "integer"
          },
          "loanId": {
            "type": "integer"
          },
          "remarks1": {
            "type": "string"
          },
          "remarks2": {
            "type": "string"
          },
          "version": {
            "type": "integer"
          }
        },
        "required": [
          "disbursementId",
          "document",
          "documentId",
          "documentStatus",
          "id",
          "loanId",
          "version"
        ]
      }
    },
    "loanAccountDisbursementSchedule": {
      "type": "object",
      "properties": {
        "accountNumber": {
          "type": "string"
        },
        "actualDisbursementDate": {
          "type": "string"
        },
        "currentStage": {
          "type": "string"
        },
        "customerAccountNumber": {
          "type": ["null", "string"]
        },
        "customerName": {
          "type": "string"
        },
        "customerSignatureDate": {
          "type": "string"
        },
        "disbursedAmount": {
          "type": "number"
        },
        "disbursementAmount": {
          "type": "number"
        },
        "disbursementFromBankAccountNumber": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "ifscCode": {
          "type": ["null", "string"]
        },
        "customerBankName": {
          "type": ["null", "string"]
        },
        "customerBankBranchName": {
          "type": ["null", "string"]
        },
        "party":{
           "type":"string"
        },
        "customerNameInBank":{
           "type":["null", "string"]
        },
        "loanId": {
          "type": "integer"
        },
        "remarks": {
          "type": "string"
        },
        "scheduledDisbursementDate": {
          "type": "string"
        },
        "trancheNumber": {
          "type": "string"
        },
        "tranchCondition": {
          "type": ["string", "null"]
        },
        "udf1": {
          "title":"DISBURSEMENT_STATUS",
          "type": "string"
        },
        "udf2": {
          "title":"REJECTION_REMARKS",
          "type": ["null", "string"],
          "enumCode": "reject_reason"
        },
        "udf3": {
          "title":"REJECTION_REASON",
          "type": ["string", "null"]
        },
        "udf4": {
          "title": "FINANCE_TEAM_REJECTION_REMARKS",
          "type": ["string", "null"]
        },
        "udf5": {
          "type": "string",
          "title":"FINANCE_TEAM_REJECTION_REASON",
          "enumCode": "reject_reason"
        },
        "udfDate1": {
          "type": "string",
          "title":"REJECTION_DATE"
        },
        "udfDate2": {
          "type": "string",
          "title":"ASSIGNED_DATE"
        },
        "udfDate3": {
          "type": "string"
        },
        "udfDate4": {
          "type": "string"
        },
        "udfDate5": {
          "type": "string"
        },
        "referenceDate": {
          "type": "string"
        },
        "referenceNumber": {
          "type": "string"
        }
      },
      "required": [
        "accountNumber",
        "actualDisbursementDate",
        "currentStage",
        "customerSignatureDate",
        "disbursedAmount",
        "disbursementAmount",
        "id",
        "loanId",
        "remarks",
        "scheduledDisbursementDate",
        "trancheNumber",
        "udf1",
        "udfDate3",
        "udfDate4",
        "udfDate5"
      ]
    },
    "stage": {
      "type": "string"
    }
  },
  "required": [
    "disbursementProcessAction",
    "individualLoanDocuments",
    "loanAccountDisbursementSchedule",
    "stage"
  ]
}