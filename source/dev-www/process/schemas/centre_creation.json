         "weeklyMeetingDay": {
            "type": ["string", "null"],
            "title": "WEEKLY MEETING DAY"
         },
         "weeklyMeetingTime": {
            "type": ["string", "null"],
            "title": "WEEEKLY MEETING TIME"
         }
