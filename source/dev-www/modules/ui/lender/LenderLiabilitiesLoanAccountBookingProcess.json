{
    "LenderAccountDetails": {
        "type": "box",
        "title": "LENDER_ACCOUNT_DETAILS",
        "items": {
            "lenderId": {
                "key": "liabilityAccount.lenderId",
                "resolver": "LenderIDLOVConfiguration",
                "type": "lov",
                "lovonly": true,
                "title": "LENDER_ID"
            },
            "lenderAccountNumber": {
                "key": "liabilityAccount.lenderAccountNumber",
                "type": "string",
                "title": "LENDER_ACCOUNT_NUMBER"
            },
            "sourcingAgent":{
                "key": "liabilityAccount.sourcingAgent",
                "type": "string",
                "title": "SOURCING_AGENT"
            }
        }
    },
    "DisbursementDetails": {
        "type": "box",
        "title": "DISBURSEMENT_DETAILS",
        "items": {
            "productType": {
                "key": "liabilityAccount.productType",
                "type": "select",
                "title": "PRODUCT_TYPE",
                "enumCode": "lender_product_type"
            },
            "loanAmount": {
                "key": "liabilityAccount.loanAmount",
                "type": "number",
                "title": "LOAN_AMOUNT"
            },
            "numberOfDisbursement": {
                "key": "liabilityAccount.numberOfDisbursement",
                "type": "number",
                "title": "NUMBER_OF_DISBURSEMENT"
            },
            "trancheNumber": {
                "key": "liabilityAccount.trancheNumber",
                "type": "string",
                "title": "TRANCHE_NUMBER"
            },
            "disbursementDate": {
                "key": "liabilityAccount.disbursementDate",
                "type": "date",
                "title": "DISBURSEMENT_DATE"
            },
            "trancheCondition": {
                "key": "liabilityAccount.trancheCondition",
                "type": "select",
                "enumCode": "tranche_conditions",
                "title": "TRANCHE_CONDITION"
            },
            "interestRateType": {
                "key": "liabilityAccount.interestRateType",
                "type": "select",
                "title": "INTEREST_RATE_TYPE",
                "enumCode": "interest_rate_type"
            },
            "rateOfInterest": {
                "key": "liabilityAccount.rateOfInterest",
                "type": "number",
                "title": "RATE_OF_INTEREST"
            },
            "markDown": {
                "key": "liabilityAccount.markDown",
                "type": "number",
                "title": "MARKDOWN"
            },
            "markUp": {
                "key": "liabilityAccount.markUp",
                "type": "number",
                "title": "MARKUP"
            },
            "interestCalculationMethod": {
                "key": "liabilityAccount.interestCalculationMethod",
                "type": "select",
                "enumCode": "interest_calculation_method",
                "title": "INTEREST_CALCULATION_METHOD"
            },
            "repaymentTenure": {
                "key": "liabilityAccount.repaymentTenure",
                "type": "number",
                "title": "REPAYMENT_TENURE"
            },
            "repaymentFrequency": {
                "key": "liabilityAccount.repaymentFrequency",
                "type": "select",
                "enumCode": "frequency",
                "title": "REPAYMENT_FREQUENCY"
            },
            "repaymentMode": {
                "key": "liabilityAccount.repaymentMode",
                "type": "select",
                "enumCode": "lender_repayment_mode",
                "title": "REPAYMENT_MODE"
            }
        }
    },
    "LoanAmountDeduction": {
        "type": "box",
        "title": "LOAN_AMOUNT_DEDUCTION",
        "items": {
            "liabilityFeeDetails": {
                "key": "liabilityAccount.liabilityFeeDetails",
                "type": "array",
                "title": "LOAN_AMOUNT_DEDUCTION",
                "items": {
                    "processingFeeInPercentage": {
                        "key": "liabilityAccount.liabilityFeeDetails[].processingFeeInPercentage",
                        "type": "number",
                        "title": "PROCESSING_FEE_IN_PERCENTAGE"
                    },
                    "feeName": {
                        "key": "liabilityAccount.liabilityFeeDetails[].feeName",
                        "type": "string",
                        "title": "FEE_NAME"
                    },
                    "feeType": {
                        "key": "liabilityAccount.liabilityFeeDetails[].feeType",
                        "type": "select",
                        "enumCode": "liability_fee_type",
                        "title": "FEE_TYPE"
                    },
                    "feeAmount": {
                        "key": "liabilityAccount.liabilityFeeDetails[].feeAmount",
                        "type": "number",
                        "title": "FEE_AMOUNT"
                    }
                }
            },
            "securityAmount": {
                "key": "liabilityAccount.securityAmount",
                "type": "number",
                "title": "SECURITY_AMOUNT"
            },
            "totalDeductions": {
                "key": "liabilityAccount.totalDeductions",
                "type": "number",
                "title": "TOTAL_DEDUCTIONS"
            },
            "netDisbursementAmount": {
                "key": "liabilityAccount.netDisbursementAmount",
                "type": "number",
                "title": "NET_DISBURSEMENT_AMOUNT"
            },
            "expectedDisbursementDate": {
                "key": "liabilityAccount.expectedDisbursementDate",
                "type": "date",
                "title": "EXPECTED_DISBURSEMENT_DATE"
            },
            "installmentAmount": {
                "key": "liabilityAccount.installmentAmount",
                "type": "number",
                "title": "INSTALLMENT_AMOUNT"
            },
            "scheduleStartDate": {
                "key": "liabilityAccount.scheduleStartDate",
                "type": "date",
                "title": "SCHEDULE_START_DATE"
            },
            "firstInstallmentDate": {
                "key": "liabilityAccount.firstInstallmentDate",
                "type": "date",
                "title": "FIRST_INSTALLMENT_DATE"
            },
            "maturityDate": {
                "key": "liabilityAccount.maturityDate",
                "type": "date",
                "title": "MATURITY_DATE"
            },
            "loanAccountStatus": {
                "key": "liabilityAccount.loanAccountStatus",
                "type": "select",
                "enumCode": "loan_account_status",
                "title": "LOAN_ACCOUNT_STATUS"
            }
        }
    },
    "LenderDocumentation": {
        "type": "box",

        "title": "LENDER_DOCUMENT",
        "items": {
            "liabilityLenderDocuments": {
                "key": "liabilityAccount.liabilityLenderDocuments",
                "type": "array",
                "title": "LENDER_DOCUMENT",
                "items": {
                    "documentName": {
                        "key": "liabilityAccount.liabilityLenderDocuments[].documentName",
                        "type": "select",
                        "enumCode": "lender_document",
                        "title": "DOCUMENT_TYPE"
                    },
                    "otherDocumentName": {
                        "key": "liabilityAccount.liabilityLenderDocuments[].otherDocumentName",
                        "type": "string",
                        "title": "DOCUMENT_NAME"
                    },
                    "upload": {
                        "title": "Upload",
                        "key": "liabilityAccount.liabilityLenderDocuments[].fileId",
                        "type": "file",
                        "fileType": "application/pdf",
                        "category": "Loan",
                        "subCategory": "DOC1",
                        "using": "scanner"
                    },
                    "isSignOff": {
                        "key": "liabilityAccount.liabilityLenderDocuments[].isSignOff",
                        "type": "radios",
                        "title": "IS_SIGNOFF",
                        "enumCode": "decisionmaker"
                    },
                    "uploadedDate": {
                        "key": "liabilityAccount.liabilityLenderDocuments[].uploadedDate",
                        "type": "date",
                        "title": "UPLOAD_DATE"
                    }
                }
            }
        }
    },
    "LegalCompliance": {
        "type": "box",
        "title": "LEGAL_COMPLIANCE",
        "items": {
            "liabilityComplianceDocuments": {
                "key": "liabilityAccount.liabilityComplianceDocuments",
                "type": "array",
                "title": "LEGAL_COMPLIANCE",
                "items": {
                    "documentName": {
                        "key": "liabilityAccount.liabilityComplianceDocuments[].documentName",
                        "type": "select",
                        "enumCode": "compliance_document",
                        "title": "DOCUMENT_TYPE"
                    },
                    "otherDocumentName": {
                        "key": "liabilityAccount.liabilityComplianceDocuments[].otherDocumentName",
                        "type": "string",
                        "title": "DOCUMENT_NAME"
                    },
                    "upload": {
                        "title": "Upload",
                        "key": "liabilityAccount.liabilityComplianceDocuments[].fileId",
                        "type": "file",
                        "fileType": "application/pdf",
                        "category": "Loan",
                        "subCategory": "DOC1",
                        "using": "scanner"
                    },
                    "isSignOff": {
                        "key": "liabilityAccount.liabilityComplianceDocuments[].isSignOff",
                        "type": "radios",
                        "title": "IS_SIGNOFF",
                        "enumCode": "decisionmaker"
                    },
                    "uploadedDate": {
                        "key": "liabilityAccount.liabilityComplianceDocuments[].uploadedDate",
                        "type": "date",
                        "title": "UPLOAD_DATE"
                    }
                }
            }
        }
    },
    "DisbursementConfirmation": {
        "type": "box",
        "title": "DISBURSEMENT_CONFIRMATION",
        "items": {
            "disbursementDate": {
                "key": "liabilityAccount.disbursementDate",
                "type": "date",
                "title": "DISBURSEMENT_DATE"
            },
            "modeOfDisbursement": {
                "key": "liabilityAccount.modeOfDisbursement",
                "type": "select",
                "enumCode": "liability_mode_of_disbursement",
                "title": "MODE_OF_DISBURSEMENT"
            },
            "referenceNumber": {
                "key": "liabilityAccount.referenceNumber",
                "type": "string",
                "title": "UTR_No"
            }
        }
    },
    "LiabilitySchedules": {
        "type": "box",
        "colClass": "col-sm-12",
        "condition": "model.liabilityAccount.liabilitySchedules && model.liabilityAccount.liabilitySchedules.length != 0",
        "title": "SCHEDULE_DETAILS",
        "items": {
            "liabilitySchedules": {
                "key": "liabilityAccount.liabilitySchedules",
                "type": "array",
                "title": "REPAYMENT_SCHEDULE_DETAILS",
                "items": {
                    "lenderAccountNumber": {
                        "key": "liabilityAccount.liabilitySchedules[].lenderAccountNumber",
                        "type": "number"
                    },
                    "installmentDueDate": {
                        "key": "liabilityAccount.liabilitySchedules[].installmentDueDate",
                        "type": "uploadedDate"
                    },
                    "paidStatus": {
                        "key": "liabilityAccount.liabilitySchedules[].paidStatus",
                        "type": "string"
                    },
                    "principalDue": {
                        "key": "liabilityAccount.liabilitySchedules[].principalDue",
                        "type": "number"
                    },
                    "interestDue": {
                        "key": "liabilityAccount.liabilitySchedules[].interestDue",
                        "type": "number"
                    },
                    "penalityDue": {
                        "key": "liabilityAccount.liabilitySchedules[].penalityDue",
                        "type": "number"
                    },
                    "otherFeeChargesDue": {
                        "key": "liabilityAccount.liabilitySchedules[].otherFeeChargesDue",
                        "type": "number"
                    },
                    "totalInstallmentAmountDue": {
                        "type": "number",
                        "key": "liabilityAccount.liabilitySchedules[].totalInstallmentAmountDue"
                    }
                }
            }
        }
    },
    "LiabilityRepayments": {
        "type": "box",
        "colClass": "col-sm-12",
        "condition": "model.liabilityAccount.liabilityRepayments && model.liabilityAccount.liabilityRepayments.length != 0",
        "title": "REPAYMENT_DETAILS",
        "items": {
            "liabilityRepayments": {
                "key": "liabilityAccount.liabilityRepayments",
                "type": "array",
                "title": "REPAYMENT_DETAILS",
                "items": {
                    "lenderAccountNumber": {
                        "key": "liabilityAccount.liabilityRepayments[].lenderAccountNumber",
                        "type": "number"
                    },
                    "installmentDate": {
                        "key": "liabilityAccount.liabilityRepayments[].installmentDate",
                        "type": "number"
                    },
                    "principalPaid": {
                        "key": "liabilityAccount.liabilityRepayments[].principalPaid",
                        "type": "number"
                    },
                    "interestPaid": {
                        "key": "liabilityAccount.liabilityRepayments[].interestPaid",
                        "type": "number"
                    },
                    "penalityPaid": {
                        "key": "liabilityAccount.liabilityRepayments[].penalityPaid",
                        "type": "number"
                    },
                    "otherFeeChargespaid": {
                        "key": "liabilityAccount.liabilityRepayments[].otherFeeChargespaid",
                        "type": "number"
                    },
                    "totalAmountPaid": {
                        "key": "liabilityAccount.liabilityRepayments[].totalAmountPaid",
                        "type": "number"
                    }
                }
            }
        }
    }
}
